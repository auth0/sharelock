{
  "platform": {
    "enabled": true
  },
  "globalValidation": {
    "requireAuthentication": true,
    "unauthenticatedClientAction": "RedirectToLoginPage",
    "redirectToProvider": "auth0",
    "excludedPaths": [
      "/"
    ]
  },
  "identityProviders": {
    "openIdConnectProviders": {
      "auth0": {
        "enabled": true,
        "registration": {
          "clientId": "6U79Ab161MT8KmYeMHCFlIM8YhyIeX35",
          "clientCredential": {
            "secretSettingName": "PRIVATE_FALLBACK_CLIENT_SECRET"
          },
          "openIdConnectConfiguration": {
            "wellKnownOpenIdConfiguration": "https://sharelock.auth0.com/.well-known/openid-configuration"
          }
        },
        "login": {
          "nameClaimType": "name",
          "scope": [
            "openid",
            "profile",
            "email"
          ],
          "loginParameterNames": []
        }
      }
    },
    "login": {
      "tokenStore": {
        "enabled": true
      },
      "preserveUrlFragmentsForLogins": true
    },
    "httpSettings": {
      "requireHttps": true
    }
  }
}